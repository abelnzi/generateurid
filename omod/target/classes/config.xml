<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.2">

	<!-- Module Properties -->
	<id>generateurid</id>
	<name>Generateur ID Module</name>
	<version>1.0-SNAPSHOT</version>
	<package>org.openmrs.module.generateurid
	</package>
	<author>Abel N'ZI</author>
	<description>
		Helps to start writing OpenMRS modules
	</description>
	<updateURL>https://modules.openmrs.org/modules/download/generateurid/update.rdf
	</updateURL>
	<require_version>1.9.0</require_version>
	<!-- / Module Properties -->


	<!-- Module Activator -->
	<activator>org.openmrs.module.generateurid.GenerateuridActivator
	</activator>


	<!-- Adds link to admin page -->
	<extension>
		<point>org.openmrs.admin.list</point>
		<class>org.openmrs.module.generateurid.extension.html.AdminList
		</class>
	</extension>

	<extension>
		<point>org.openmrs.gutter.tools</point>
		<class>org.openmrs.module.generateurid.extension.html.GenerateurIdGutterLink
		</class>
	</extension>

	<!-- Maps hibernate file's, if present -->
	<mappingFiles>
		GeneratedId.hbm.xml
	</mappingFiles>

	<!-- Privileges list -->
	<privilege>
		<name>Manage Generateur ID</name>
		<description>Can Manage Generateur IDs for SIGDEP-2</description>
	</privilege>

	<!-- Internationalization -->
	<!-- All message codes should start with generateurid. -->
	<messages>
		<lang>en</lang>
		<file>messages.properties</file>
	</messages>
	<messages>
		<lang>fr</lang>
		<file>messages_fr.properties</file>
	</messages>
	<messages>
		<lang>es</lang>
		<file>messages_es.properties</file>
	</messages>
	<!-- /Internationalization -->

</module>

